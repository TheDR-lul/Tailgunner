{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Tailgunner",
    "version": "0.8.0",
  "identifier": "com.tailgunner.ass",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Tailgunner - powered by A.S.S.",
        "width": 1280,
        "height": 720,
        "minWidth": 1024,
        "minHeight": 576,
        "decorations": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
